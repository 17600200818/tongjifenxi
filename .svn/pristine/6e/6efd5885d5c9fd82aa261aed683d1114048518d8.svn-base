[stat]

;; logpath 要分析的日志目录
logpath = ./tmp/stat/source

;; cachePath 分析用到的缓存目录
tmppath = ./tmp/stat/cache

;; outpath 分析后结果存放目录
outpath = ./tmp/stat/result

;; errpath 错误日志存放目录
errpath = ./tmp/stat/error

;; 处理完的日志备份
bakpath = ./tmp/stat/bak

;; 一次执行读日志文件个数
readFileNum = 100

;; redis集群
redis = 127.0.0.1:6376

[service]
;; 程序每次执行的超时时间
timeout	= 3600

